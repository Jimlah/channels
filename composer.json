{
    "name": "laravel-notification-channels/smsto",
    "description": "SMSTo Notification Channel for Laravel",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "notification",
        "channel",
        "sms",
        "smsto"
    ],
    "homepage": "https://github.com/laravel-notification-channels/smsto",
    "license": "MIT",
    "authors": [{
        "name": "Abdullahi Jimoh",
        "email": "abdullahij951@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\SMSTo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\SMSTo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
